<?php

/**
 * @file
 * Contains advanced_select.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function advanced_select_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the advanced_select module.
    case 'help.page.advanced_select':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Advanced select widget for list type field') . '</p>';
      return $output;

    default:
  }
}
